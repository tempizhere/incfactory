# Kaiten settings
KAITEN_HOST=https://example.kaiten.ru
KAITEN_API_KEY=your_kaiten_api_key
SPACES=101,102
OPEN_CARD_COLUMNS=1,2
CLOSED_CARD_COLUMNS=3,4
LIMIT=100
EXCLUDED_COMMENT_AUTHORS=bot

# Database settings
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_db_password
DB_NAME=incfactory

# Sender email for Air-Run
SENDER_EMAIL=your_email@example.com

# LLM/Embedding Type (FALSE for OpenAI-like)
DIRECT_LLM=TRUE

# Embedding settings (Primary LLM API)
LLM_PRIMARY_HOST=https://example.api.com
LLM_PRIMARY_API_KEY=your_primary_llm_api_key
LLM_PRIMARY_PROVIDER=openai
LLM_PRIMARY_MODEL=text-embedding-ada-002
EMBEDDING_MAX_TEXT_LENGTH=10000
EMBEDDING_RATE_LIMIT=3
EMBEDDING_MAX_RETRIES=3
EMBEDDING_RETRY_DELAY=1000

# Backup Embedding settings
LLM_BACKUP_HOST=https://backup.example.api.com
LLM_BACKUP_API_KEY=your_backup_llm_api_key
LLM_BACKUP_PROVIDER=huggingface
LLM_BACKUP_MODEL=sentence-transformers

# LLM settings
LLM_HOST=https://example.api.com
LLM_API_KEY=your_llm_api_key
LLM_PROVIDER=x5
LLM_MODEL=x5-airun-large-prod
LLM_RATE_LIMIT=3
LLM_MAX_RETRIES=3
LLM_RETRY_DELAY=10
LLM_MAX_TOKENS=10000
LLM_DEFAULT_TEMPERATURE=0.7
LLM_JSON_PARSE_RETRIES=3

# RabbitMQ settings
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=your_rabbitmq_password
RABBITMQ_QUEUE_KAITEN_NORMALIZED=kaiten-normalized
RABBITMQ_QUEUE_KAITEN_EMBEDDINGS=kaiten-embeddings
RABBITMQ_QUEUE_KAITEN_SEARCH=kaiten-search
RABBITMQ_QUEUE_KAITEN_SEARCH_RESULTS=kaiten-search-results
RABBITMQ_QUEUE_KAITEN_DATA_REQUEST=kaiten-data-request
RABBITMQ_QUEUE_KAITEN_DATA_RESPONSE=kaiten-data-response
RABBITMQ_QUEUE_KAITEN_COMMENT=kaiten-comment
RABBITMQ_QUEUE_KAITEN_COMMENT_FETCH=kaiten-comment-fetch
RABBITMQ_DEAD_LETTER_TTL=3600

# Logging settings
LOG_LEVEL=info
LOG_FORMAT=json