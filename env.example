# Внешние зависимости - ПОЛНЫЕ настройки
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=rmuser
RABBITMQ_PASS=rmpassword

DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=password
DB_NAME=incfactory

# LLM конфигурация
EMBEDDING_HOST=https://api-copilot.x5.ru/direct-llm/v1/embeddings
EMBEDDING_API_KEY=your_key
EMBEDDING_PROVIDER=x5-embed
EMBEDDING_MODEL=x5-airun-multilingual-e5-large

LLM_HOST=https://api-copilot.x5.ru/direct-llm/v1/chat/completions
LLM_API_KEY=your_key
LLM_PROVIDER=x5
LLM_MODEL=x5-airun-multilingual-e5-large

# Rate limits
EMBEDDING_RATE_LIMIT=10
LLM_RATE_LIMIT=5
LLM_MAX_RETRIES=3
EMBEDDING_MAX_RETRIES=3
LLM_RETRY_DELAY=1.0
EMBEDDING_RETRY_DELAY=1.0
EMBEDDING_MAX_TEXT_LENGTH=1000
LLM_MAX_TOKENS=10000
LLM_JSON_PARSE_RETRIES=3
RABBITMQ_MAX_DELIVERY_RETRIES=5

# Kaiten конфигурация (для fetcher)
KAITEN_HOST=https://kaiten.io
KAITEN_API_KEY=your_key
SPACE_ID=your_space_id
COLUMN_ID=your_column_id
LIMIT=100
EXCLUDED_COMMENT_AUTHORS=bot,automation
KAITEN_RATE_LIMIT_RPS=1.0
